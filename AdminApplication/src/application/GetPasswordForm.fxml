<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GetPasswordController">
   <children>
      <Label layoutX="105.0" layoutY="14.0" text="Get password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="122.0" layoutY="101.0" text="Username:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="75.0" layoutY="135.0" prefHeight="30.0" prefWidth="180.0" promptText="Enter username:">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordField" editable="false" layoutX="75.0" layoutY="190.0" prefHeight="30.0" prefWidth="180.0" visible="false">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <Button fx:id="getBtn" layoutX="122.0" layoutY="246.0" mnemonicParsing="false" onAction="#getOne" prefHeight="30.0" prefWidth="87.0" text="Get">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
